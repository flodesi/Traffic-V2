YUI.add("flickr-photos-getGroupInvitations-fetcher",function(e,t){"use strict";function n(t,n){var r=t[0],i=t[1],s=t[2],o=t[3],u=t[4],a=t[5],f=t[6],l=r.groups&&r.groups.group,c=n.id,h={},p=[],d=[],v=[],m;l&&e.Array.each(l,function(e,t){var r,i,a,f,l,c;r=[n.id,e.id].join("-"),i={id:r,photoId:n.id,groupId:e.id,invitedBy:e.invitedBy.nsid},a=s.addOrUpdate(i),f=u.parseGroupModel(n.id,n.pathAlias,e),l=u.addOrUpdate(f),c=o.addOrUpdate({id:e.id,pathAlias:e.pathAlias,group:l}),p.push(a),d.push(c)}),r&&r.pendingInvites&&e.Array.each(r.pendingInvites,function(e,t){var r=[n.id,e.groupNsid].join("-"),i={id:r,photoId:n.id,groupId:e.groupNsid};v.push(f.addOrUpdate(i))}),m={id:c,pendings:v,total:v.length},a.addOrUpdate(m),h={id:c,groups:d,invitations:p,total:p.length},i.addOrUpdate(h)}function r(n,r){var i=this;return e.Promise.all([r.callAPI("flickr.photos.getGroupInvitations",{photo_id:n.id,extras:e.APIHelper.request.getRebootGroupExtras()+", icon_urls",hermes:"1",include_pending:1}),r.getModelRegistry("photo-group-invitations-models"),r.getModelRegistry("photo-group-invitation-models"),r.getModelRegistry("group-pool-models"),r.getModelRegistry("group-models"),r.getModelRegistry("photo-group-pendings-models"),r.getModelRegistry("photo-group-pending-models")]).then(function(e){i._processResponse(e,n)},e.FetcherErrorLogger(t))}e.namespace("ListFetchers")["flickr-photos-getGroupInvitations"]={run:r,_processResponse:n}},"@VERSION@",{requires:["flickr-promise","api-helper"],optional:["photo-group-invitations-models","group-models","group-pool-models","person-models","photo-group-invitation-models"]});
YUI.add("flickr-groups-invite-photo-accept-updater",function(e,t){"use strict";function n(n,r){var i={},s=e.Models["group-pool-models"].extractGroupFromCompoundId(n.groupID);return i.user_id=n.userID,i.photo_id=n.photoID,i.group_id=s,e.Promise.all([r.callAPI("flickr.groups.invite.photo.accept",i),r.getModel("group-pool-models",s),r.getModelRegistry("photo-group-invitation-models"),r.getModelRegistry("photo-group-invitations-models"),r.getModel("photo-models",n.photoID),r.getModelRegistry("photo-groups-models"),r.getModelRegistry("photo-contexts-models")]).then(function(e){var t=e[0],r=e[1],i=e[2],o=e[3],u=e[4],a=e[5],f=e[6],l;if(t.stat==="ok")return r.addPhotoLocal(u),r.getValue("group").setValue("poolCount",r.getValue("group").getValue("poolCount")+1),a.exists(n.photoID)&&!a.getValue(n.photoID,"groups").getFromListByID(r.getValue("id"))&&(a.getValue(n.photoID,"groups").prependToList(r),a.setValue(n.photoID,"total",a.getValue(n.photoID,"total")+1)),f.exists(n.photoID)&&!f.getValue(n.photoID,"contexts").getFromListByID(r.getValue("id"))&&(f.setValue(n.photoID,"totalGroups",f.getValue(n.photoID,"totalGroups")+1),f.getValue(n.photoID,"contexts").appendToList(r)),l=[n.photoID,s].join("-"),o.exists(n.photoID)&&o.getValue(n.photoID,"invitations").getFromListByID(l)&&(o.setValue(n.photoID,"total",o.getValue(n.photoID,"total")-1),o.getValue(n.photoID,"groups").removeFromList(s),o.getValue(n.photoID,"invitations").removeFromList(l)),i.remove(l),r}).then(null,e.FetcherErrorLogger(t))}e.namespace("ModelUpdaters")["flickr-groups-invite-photo-accept"]={run:n}},"@VERSION@",{requires:["flickr-promise","group-pool-models"],optional:[]});
